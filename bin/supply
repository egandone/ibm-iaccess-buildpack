#!/bin/bash
set -euo pipefail

set -x 

BUILD_DIR=$1
CACHE_DIR=$2
DEPS_DIR=$3
DEPS_IDX=$4

mkdir -p $BUILD_DIR/.apt

ls -l

export BUILDPACK_DIR=`dirname $(readlink -f ${BASH_SOURCE%/*})`
echo -e "buildpack_dir = $BUILDPACK_DIR"

curl -I https://raw.githubusercontent.com/egandone/ibm-iaccess-buildpack/main/ibm-iaccess-1.1.0.15-1.0.amd64.deb

dpkg -x ibm-iaccess-1.1.0.15-1.0.amd64.deb $BUILD_DIR/.apt/
